{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "lib": ["es6"],
      "strict": true,
      "noImplicitAny": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "baseUrl": ".",
      "paths": {
        "@/*": ["../*"]
      }
    },
    "include": ["seed/**/*"],
    "exclude": ["node_modules"]
  }